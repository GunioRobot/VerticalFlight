@model IEnumerable<Web.UI.Models.AirmanCertificateCreationModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>ApplicationID</th>
        <th>ClassCategory</th>
        <th>Ratings</th>
        <th>DateObtained</th>
        <th>AircraftType</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AirmanCertificateID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AirmanCertificateID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AirmanCertificateID })
        </td>
        <td>@item.ApplicationID</td>
        <td>@item.ClassCategory</td>
        <td>@item.Ratings</td>
        <td>@String.Format("{0:g}", item.DateObtained)</td>
        <td>@item.AircraftType</td>
    </tr>
}

</table>

