<!DOCTYPE html>
<html>
<head>
  <title>@ViewBag.Title</title>
  <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
  <link href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/themes/base/jquery-ui.css" rel="stylesheet" type="text/css"/>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.5/jquery.min.js"></script>
  <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8/jquery-ui.min.js"></script>
  <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.2.2.js")"></script>
  <script src="@Url.Content("~/Scripts/ui_helper.js")" type="text/javascript"></script>

  	
</head>
<body>
  <div id="wrapper">
    <div id="page">
      <div id="header">
        <div id="login-display">
          @Html.Partial("_LogOnPartial")
        </div>
      </div>
      <div id="menu-bar">
        <ul id="menu">
          <li>@Html.ActionLink("Home", "Index", "Home")</li>
          <li>@Html.ActionLink("About", "About", "Home")</li>
        </ul>
      </div>
      <div id="content">
        @RenderBody()
      </div>
    </div>
    <div id="footer">
        VerticalFlight.org &copy; 2010 <br />
        Hosted by <a href="http://discountasp.net/" target="_blank">discount ASP.NET</a>
    </div>
  </div>
</body>
</html>
