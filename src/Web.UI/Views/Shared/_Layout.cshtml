<!DOCTYPE html>
<html>
<head>
  <title>@ViewBag.Title</title>
  
  <link href="@Url.Content("~/Content/style.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/telerik.common.min.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/telerik.default.min.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/telerik.telerik.min.css")" rel="stylesheet" type="text/css" />
  <link href="@Url.Content("~/Content/jquery-ui.css")" rel="stylesheet" type="text/css" />
  
  <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/jquery-ui.min.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/jquery.ui.core.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/jquery.maskedinput-1.2.2.js")" type="text/javascript"></script>
  <script src="@Url.Content("~/Scripts/ui_helper.js")" type="text/javascript"></script>
   
   <script src="@Url.Content("~/Scripts/telerik.common.min.js")" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Scripts/telerik.editor.min.js")" type="text/javascript"></script>
    
    <script src="@Url.Content("~/Scripts/telerik.grid.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.window.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/telerik.autocomplete.min.js")" type="text/javascript"></script>
   
    <script src="@Url.Content("~/Scripts/telerik.calendar.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.combobox.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.datepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.datetimepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.draganddrop.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.grid.editing.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.grid.filtering.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.grid.grouping.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.grid.reordering.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.grid.resizing.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.imagebrowser.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.list.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.menu.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.panelbar.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.slider.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.splitter.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.tabstrip.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.textbox.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.timepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.treeview.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/telerik.upload.min.js")" type="text/javascript"></script>
  	
</head>
<body>
  <div id="wrapper">
    <div id="page">
      <div id="header">
        <div id="login-display">
          @Html.Partial("_LogOnPartial")
        </div>
      </div>
      <div id="menu-bar">
        <ul id="menu">
          <li>@Html.ActionLink("Home", "Index", "Home")</li>
          <li>@Html.ActionLink("Apply For A Scholarship", "Index", "Scholarship" )</li>
        </ul>
      </div>
      <div id="content">
        @RenderBody()
      </div>
    </div>
    <div id="footer">
        VerticalFlight.org &copy; 2010 :: Hosted by <a href="http://discountasp.net/" target="_blank">Discount ASP.NET</a>
    </div>
  </div>
@(Html.Telerik().ScriptRegistrar().DefaultGroup(group => group.Compress(true)).jQuery(false))
</body>
</html>
